{
  "expo": {
    "name": "family-care-app",
    "slug": "family-care-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo.png",
    "scheme": "familycareapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.croon",
      "config": {
        "googleMapsApiKey": "AIzaSyC5nmeM5e4zyw9gBPOCqVV0HVa0wKAVZKA"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.croon",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/img/logo.png",
        "backgroundImage": "./assets/img/logo.png",
        "monochromeImage": "./assets/img/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
