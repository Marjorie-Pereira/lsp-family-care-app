{
  "expo": {
    "name": "family-care-app",
    "slug": "family-care-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/splash-icon.png",
    "scheme": "familycareapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/img/splash-icon.png",
        "backgroundImage": "./assets/img/splash-icon.png",
        "monochromeImage": "./assets/img/splash-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoibWFyam9yaWVwZXJlaXJhIiwiYSI6ImNtaGk1YzNzejB6emoyanBxOXJrM21hcGYifQ.zc4zvue6tb7MC9Na3uVhCg"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
